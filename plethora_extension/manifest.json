{
  "name": "Plethora",
  "description": "Store shared bookmarks",
  "version": "0.3",
  "permissions": [
    "activeTab",
    "http://localhost:3000/*"
  ],
  "background": {
    "scripts": ["jquery.min.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Store shared bookmarks"
  },
  "manifest_version": 2
}
